{
    "rewrites": [
      {
        "source": "/react-stocks-dashboard",
        "destination": "https://react-stocks-dashboard.vercel.app/react-stocks-dashboard"
      },
      {
        "source": "/react-stocks-dashboard/:path*",
        "destination": "https://react-stocks-dashboard.vercel.app/react-stocks-dashboard/:path*"
      },
      {
        "source": "/react-stocks-dashboard/_next/:path*",
        "destination": "https://react-stocks-dashboard.vercel.app/_next/:path*"
      },
      {
        "source": "/_next/:path*",
        "destination": "https://react-stocks-dashboard.vercel.app/_next/:path*"
      },
      {
        "source": "/react-stocks-dashboard/api/stocks/:path*",
        "destination": "https://react-stocks-dashboard.vercel.app/api/stocks/:path*"
      },
      {
        "source": "/react-stocks-dashboard/api/search",
        "destination": "https://react-stocks-dashboard.vercel.app/api/search"
      },
      {
        "source": "/react-stocks-dashboard/api/ws/:path*",
        "destination": "https://react-stocks-dashboard.vercel.app/api/ws/:path*"
      },
      {
        "source": "/react-stocks-dashboard/api/:path*",
        "destination": "https://react-stocks-dashboard.vercel.app/api/:path*"
      },
      {
        "source": "/vue-stocks-dashboard",
        "destination": "https://stocks-dashboard-vert.vercel.app"
      },
      {
        "source": "/vue-stocks-dashboard/:path*",
        "destination": "https://stocks-dashboard-vert.vercel.app/:path*"
      }
    ],
    "headers": [
      {
        "source": "/vue-stocks-dashboard/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/vue-stocks-dashboard/(.*)\\.js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript; charset=utf-8"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/vue-stocks-dashboard/(.*)\\.css",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/css; charset=utf-8"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/vue-stocks-dashboard/(.*)\\.html",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/html; charset=utf-8"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=0, must-revalidate"
          }
        ]
      }
    ]
}
  
  